{
  "name": "xynginc",
  "version": "1.0.0",
  "description": "XyPriss Nginx Controller - Automatic Nginx & SSL management for XyPriss servers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "xynginc": "./bin/xynginc"
  },
  "scripts": {
    "build": "tsc",
    "build:rust": "cd rust && cargo build --release && cp target/release/xynginc ../bin/",
    "build:all": "npm run build:rust && npm run build",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "postinstall": "node scripts/postinstall.js"
  },
  "keywords": [
    "xypriss",
    "nginx",
    "ssl",
    "certbot",
    "reverse-proxy",
    "letsencrypt",
    "plugin"
  ],
  "author": "iDevo",
  "license": "MIT",
  "peerDependencies": {
    "xypriss": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "files": [
    "dist",
    "bin",
    "scripts",
    "README.md"
  ],
  "os": [
    "linux"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Nehonix-Team/xynginc.git"
  },
  "bugs": {
    "url": "https://github.com/Nehonix-Team/xynginc.git/issues"
  }
}
